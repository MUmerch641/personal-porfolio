
{
    "version": 2,
    "builds": [
      {
        "src": "./server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/contact",
        "dest": "./server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "./server/server.js"
      }
    ]
  }